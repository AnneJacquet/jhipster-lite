{
  "extension": [".js", ".cjs", ".mjs", ".ts", ".tsx"],
  "reporter": ["html", "json", "lcov", "text-summary"],
  "report-dir": "{{ projectBuildDirectory }}/frontend-coverage/combined",
  "temp-dir": "{{ projectBuildDirectory }}/.nyc_output",
  "check-coverage": true,
  "branches": 70,
  "lines": 70,
  "functions": 70,
  "statements": 70,
  "exclude": ["src/test/**/*"]
}
